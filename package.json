{
  "name": "@types/bevi-icon-react",
  "private": false,
  "version": "1.0.0",
  "description": "Biblioteca de ícones da Bevi para projetos React",
  "license": "MIT",
  "author": {
    "name": "André Luiz",
    "email": "andre.aguiar@bevicred.com.br"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BeviProjects/BeviIconReact.git"
  },
  "bugs": {
    "url": "https://github.com/BeviProjects/BeviIconReact/issues"
  },
  "homepage": "https://github.com/BeviProjects/BeviIconReact#readme",
  "keywords": [
    "bevi",
    "react",
    "typescript",
    "icon library"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/bevi-icon.umd.js",
  "module": "./dist/bevi-icon.es.js",
  "exports": {
    ".": {
      "import": "./dist/bevi-icon.es.js",
      "require": "./dist/bevi-icon.umd.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "module-name": "https://github.com/BeviProjects/BeviIconReact/releases/tag/v1.0.zip"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest run",
    "watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es2020": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "devDependencies": {
    "@testing-library/dom": "^8.20.0",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "c8": "^7.12.0",
    "eslint": "^8.34.0",
    "eslint-plugin-react": "^7.32.2",
    "jsdom": "^21.1.0",
    "react-test-renderer": "^18.2.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vitest": "^0.28.5"
  }
}
